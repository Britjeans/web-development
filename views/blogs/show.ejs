<% include ../partials/header %> 



<% include ../functions %> 

<div class="container">
	<div class="blog-show">
		<div class="blog-title">
				<h2><%= blog.title %></h2>
		</div>
		<div class="blog-author">
			<a href="/blogs/author/<%= blog.author_id %>">
				<%= blog.author_name %>
			</a>
		</div>
		<span> <%= blog.create %> </span>
		<div class="blog-content">
			<%- blog.content %>
		</div>
	</div>
</div>


<% include ../partials/footer %> 