		    </form>
		  </div>
		</nav>
<div id="calendar-features">
	<div class="to-do-list">
		<div class="planner-header">
			<h2>To Do List</h2>
			<h4><%= year %>-<%= month %>-<%= day %></h4>
		</div>
		<form action="\planner" method="POST">
			<input type="text" name="todo">
			<input type="submit" class="btn btn-primary" style="display: inline;margin-left: 10px; border-color: #F44082; background-color: #F3568F;">
		</form>
		<div id="event-list">
			<% if(plans) { %>
			<% plans.content.forEach(function(block) { %>
				<div class="row">
					<div calss="col">
						<div class="plan-content"><%= block.plan %></div>
						<form class="plan-form"  method="POST" action="/plans/<%= plans._id %>/delete/<%= block._id %>?_method=DELETE">
							<input type="submit" value="delete" class="btn btn-sm btn-primary" style="border-color: #F44082; background-color: #F3568F;">
						</form>
					</div>
				</div>
			<% });} %>

		</div>
	</div>


